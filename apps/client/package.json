{
  "name": "multi-agent-observability-client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:regression": "vitest run tests/regression",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:edge": "playwright test --project=msedge",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:cross-browser": "playwright test --project=chromium --project=firefox --project=webkit --project=msedge",
    "test:performance": "playwright test --project=performance",
    "test:accessibility": "playwright test --project=accessibility",
    "test:visual": "playwright test --project=visual",
    "test:install-browsers": "playwright install",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "@babel/types": "^7.28.2",
    "@guolao/vue-monaco-editor": "^1.5.5",
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "mitt": "^3.0.1",
    "puppeteer": "^24.17.0",
    "pyodide": "^0.28.2",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^22.11.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.8.3",
    "vite": "^6.3.6",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  }
}
